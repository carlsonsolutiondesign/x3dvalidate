    "Scene": {
        "-children":[
          { "Viewpoint":
            {
              "@description":"Outside (Front)",
              "@position":[0,0,12]
            }
          },
          { "Viewpoint":
            {
              "@description":"Inside Front",
              "@position":[0,0,-5]
            }
          },
          { "Group":
            {
              "-children":[
                { "TimeSensor":
                  {
                    "@DEF":"TIME",
                    "@cycleInterval":2,
                    "@loop":true,
                    "@stopTime":1
                  }
                },
                { "ColorInterpolator":
                  {
                    "@DEF":"INTERP_COLOR",
                    "@key":[0,0.5,1],
                    "@keyValue":[1,0,0,0,1,0,0,0,1]
                  }
                },
                { "VisibilitySensor":
                  {
                    "@DEF":"VIS_SENSOR",
                    "@size":[4,4,4]
                  }
                },
                { "Transform":
                  {
                    "-children":[
                      { "Shape":
                        {
                          "-appearance":
                            { "Appearance":
                              {
                                "-material":
                                  { "Material":
                                    {
                                      "@emissiveColor":[1,1,1]
                                    }
                                  }
                              }
                            },
                          "-geometry":
                            { "IndexedLineSet":
                              {
                                "@coordIndex":[0,1,-1,1,2,-1,2,3,-1,3,0,-1,4,5,-1,5,6,-1,6,7,-1,7,4,-1,1,5,-1,2,6,-1,0,4,-1,7,3,-1],
                                "-coord":
                                  { "Coordinate":
                                    {
                                      "@point":[-2,-2,-2,2,-2,-2,2,2,-2,-2,2,-2,-2,-2,2,2,-2,2,2,2,2,-2,2,2]
                                    }
                                  }
                              }
                            }
                        }
                      }
                    ]
                  }
                },
                { "Transform":
                  {
                    "@DEF":"GEOMETRY",
                    "@translation":[0,0,-15],
                    "-children":[
                      { "Shape":
                        {
                          "-appearance":
                            { "Appearance":
                              {
                                "-material":
                                  { "Material":
                                    {
                                      "@DEF":"COLOR",
                                      "@diffuseColor":[1,0,0]
                                    }
                                  }
                              }
                            },
                          "-geometry":
                            { "Sphere":
                              {
                                "@radius":2
                              }
                            }
                        }
                      }
                    ]
                  }
                }
              ]
            }
          },
          { "ROUTE":
            {
              "@fromField":"enterTime",
              "@fromNode":"VIS_SENSOR",
              "@toField":"startTime",
              "@toNode":"TIME"
            }
          },
          { "ROUTE":
            {
              "@fromField":"fraction_changed",
              "@fromNode":"TIME",
              "@toField":"set_fraction",
              "@toNode":"INTERP_COLOR"
            }
          },
          { "ROUTE":
            {
              "@fromField":"value_changed",
              "@fromNode":"INTERP_COLOR",
              "@toField":"diffuseColor",
              "@toNode":"COLOR"
            }
          },
          { "ROUTE":
            {
              "@fromField":"exitTime",
              "@fromNode":"VIS_SENSOR",
              "@toField":"stopTime",
              "@toNode":"TIME"
            }
          }
        ]
    }
  }
