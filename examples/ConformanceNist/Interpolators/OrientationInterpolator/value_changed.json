    "Scene": {
        "-children":[
          { "OrientationInterpolator":
            {
              "@DEF":"MOVER1",
              "@key":[0,0.5,1],
              "@keyValue":[0,0,1,0.785,0,0,1,0.785,0,0,1,0.785]
            }
          },
          { "OrientationInterpolator":
            {
              "@DEF":"MOVER2"
            }
          },
          { "Transform":
            {
              "@translation":[-2,0,0],
              "-children":[
                { "Transform":
                  {
                    "@DEF":"TRANS1",
                    "-children":[
                      { "TouchSensor":
                        {
                          "@DEF":"TS1",
                          "@description":"touch to activate"
                        }
                      },
                      { "Shape":
                        {
                          "-appearance":
                            { "Appearance":
                              {
                                "-material":
                                  { "Material":
                                    {
                                      "@diffuseColor":[1,0,0]
                                    }
                                  }
                              }
                            },
                          "-geometry":
                            { "Box":
                              {
                              }
                            }
                        }
                      }
                    ]
                  }
                }
              ]
            }
          },
          { "Transform":
            {
              "@translation":[2,0,0],
              "-children":[
                { "Transform":
                  {
                    "@DEF":"TRANS2",
                    "-children":[
                      { "TouchSensor":
                        {
                          "@DEF":"TS2",
                          "@description":"touch to activate"
                        }
                      },
                      { "Shape":
                        {
                          "-appearance":
                            { "Appearance":
                              {
                                "-material":
                                  { "Material":
                                    {
                                      "@diffuseColor":[1,0,0]
                                    }
                                  }
                              }
                            },
                          "-geometry":
                            { "Box":
                              {
                              }
                            }
                        }
                      }
                    ]
                  }
                }
              ]
            }
          },
          { "Script":
            {
              "@DEF":"MOVER",
              "@directOutput":true,
              "@url":["value_changed.js","http://www.web3d.org/x3d/content/examples/ConformanceNist/Interpolators/OrientationInterpolator/value_changed.js"],
              "field": [
                {
                  "@name":"nodein1",
                  "@accessType":"initializeOnly",
                  "@type":"SFNode",
                  "-children":[
                    { "OrientationInterpolator":
                      {
                        "@USE":"MOVER1"
                      }
                    }
                  ]
                },
                {
                  "@name":"nodein2",
                  "@accessType":"initializeOnly",
                  "@type":"SFNode",
                  "-children":[
                    { "OrientationInterpolator":
                      {
                        "@USE":"MOVER2"
                      }
                    }
                  ]
                },
                {
                  "@name":"nodeout1",
                  "@accessType":"initializeOnly",
                  "@type":"SFNode",
                  "-children":[
                    { "Transform":
                      {
                        "@USE":"TRANS1"
                      }
                    }
                  ]
                },
                {
                  "@name":"nodeout2",
                  "@accessType":"initializeOnly",
                  "@type":"SFNode",
                  "-children":[
                    { "Transform":
                      {
                        "@USE":"TRANS2"
                      }
                    }
                  ]
                },
                {
                  "@name":"reset",
                  "@accessType":"initializeOnly",
                  "@type":"SFRotation",
                  "@value":[0,0,10]
                },
                {
                  "@name":"isOver1",
                  "@accessType":"inputOnly",
                  "@type":"SFBool"
                },
                {
                  "@name":"isOver2",
                  "@accessType":"inputOnly",
                  "@type":"SFBool"
                },
                {
                  "@name":"rotate",
                  "@accessType":"outputOnly",
                  "@type":"SFRotation"
                }
              ]
            }
          },
          { "ROUTE":
            {
              "@fromField":"isOver",
              "@fromNode":"TS1",
              "@toField":"isOver1",
              "@toNode":"MOVER"
            }
          },
          { "ROUTE":
            {
              "@fromField":"isOver",
              "@fromNode":"TS2",
              "@toField":"isOver2",
              "@toNode":"MOVER"
            }
          }
        ]
    }
  }
