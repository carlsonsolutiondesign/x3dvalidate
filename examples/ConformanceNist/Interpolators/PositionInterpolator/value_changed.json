                                    }
                                  }
                              }
                            },
                          "-geometry":
                            { "Box":
                              {
                              }
                            }
                        }
                      }
                    ]
                  }
                }
              ]
            }
          },
          { "Transform":
            {
              "@translation":[2,0,0],
              "-children":[
                { "Transform":
                  {
                    "@DEF":"TRANS2",
                    "-children":[
                      { "TouchSensor":
                        {
                          "@DEF":"TS2",
                          "@description":"touch to activate"
                        }
                      },
                      { "Shape":
                        {
                          "-appearance":
                            { "Appearance":
                              {
                                "-material":
                                  { "Material":
                                    {
                                      "@diffuseColor":[1,0,0]
                                    }
                                  }
                              }
                            },
                          "-geometry":
                            { "Box":
                              {
                              }
                            }
                        }
                      }
                    ]
                  }
                }
              ]
            }
          },
          { "Script":
            {
              "@DEF":"MOVEIT",
              "@directOutput":true,
              "@url":["value_changed.js","http://www.web3d.org/x3d/content/examples/ConformanceNist/Interpolators/PositionInterpolator/value_changed.js"],
              "field": [
                {
                  "@name":"nodein1",
                  "@accessType":"initializeOnly",
                  "@type":"SFNode",
                  "-children":[
                    { "PositionInterpolator":
                      {
                        "@USE":"MOVER1"
                      }
                    }
                  ]
                },
                {
                  "@name":"nodein2",
                  "@accessType":"initializeOnly",
                  "@type":"SFNode",
                  "-children":[
                    { "PositionInterpolator":
                      {
