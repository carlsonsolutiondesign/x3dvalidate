    "Scene": {
        "-children":[
          { "NavigationInfo":
            {
              "@type":["EXAMINE","WALK","FLY","ANY"]
            }
          },
          { "TimeSensor":
            {
              "@DEF":"TIME",
              "@cycleInterval":5,
              "@loop":true
            }
          },
          { "PositionInterpolator":
            {
              "@DEF":"POSITION",
              "@key":[0,0.08,0.16,0.24,0.32,0.4,0.48,0.56,0.64,0.72,0.8,0.88,0.96],
              "@keyValue":[0,0,0,-2,0,0,0,0,0,2,0,0,0,0,0,0,-2,0,0,0,0,0,2,0,0,0,0,0,0,-2,0,0,0,0,0,2,0,0,0]
            }
          },
          { "Group":
            {
              "-children":[
                { "Transform":
                  {
                    "@DEF":"TRANS",
                    "-children":[
                      { "Shape":
                        {
                          "-appearance":
                            { "Appearance":
                              {
                                "-material":
                                  { "Material":
                                    {
                                      "@DEF":"COLOR",
                                      "@diffuseColor":[1,0,0]
                                    }
                                  }
                              }
                            },
                          "-geometry":
                            { "Box":
                              {
                              }
                            }
                        }
                      }
                    ]
                  }
                }
              ]
            }
          },
          { "ROUTE":
            {
              "@fromField":"fraction_changed",
              "@fromNode":"TIME",
              "@toField":"set_fraction",
              "@toNode":"POSITION"
            }
          },
          { "ROUTE":
            {
              "@fromField":"value_changed",
              "@fromNode":"POSITION",
              "@toField":"set_translation",
              "@toNode":"TRANS"
            }
          }
        ]
    }
  }
