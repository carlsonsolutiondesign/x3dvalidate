                            { "Appearance":
                              {
                                "-material":
                                  { "Material":
                                    {
                                    }
                                  }
                              }
                            },
                          "-geometry":
                            { "Text":
                              {
                                "@DEF":"TYPESTRING0",
                                "@string":[""],
                                "-fontStyle":
                                  { "FontStyle":
                                    {
                                      "@style":"BOLD"
                                    }
                                  }
                              }
                            }
                        }
                      }
                    ]
                  }
                }
              ]
            }
          },
          { "Transform":
            {
              "@DEF":"TRANS1",
              "-children":[
                { "Transform":
                  {
                    "@translation":[-8,-6,-12],
                    "-children":[
                      { "Shape":
                        {
                          "-appearance":
                            { "Appearance":
                              {
