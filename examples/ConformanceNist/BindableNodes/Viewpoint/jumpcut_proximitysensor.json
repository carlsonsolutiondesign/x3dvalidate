                              {
                                "@DEF":"BOX1",
                                "@diffuseColor":[1,0,0]
                              }
                            }
                        }
                      },
                    "-geometry":
                      { "Box":
                        {
                          "@size":[3,3,3]
                        }
                      }
                  }
                }
              ]
            }
          },
          { "Transform":
            {
              "@DEF":"TRANS0",
              "-children":[
                { "Transform":
                  {
                    "@translation":[-8,-5,-12],
                    "-children":[
                      { "Shape":
                        {
