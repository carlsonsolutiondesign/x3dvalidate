    "Scene": {
        "-children":[
          {
            "#comment":"Click and drag to rotate shapes. Superfluous Group nodes removed."
          },
          { "Transform":
            {
              "@DEF":"FIRST_TRANSFORM",
              "@translation":[-2,0,0],
              "-children":[
                { "Shape":
                  {
                    "-geometry":
                      { "Box":
                        {
                        }
                      },
                    "-appearance":
                      { "Appearance":
                        {
                          "@DEF":"FIRST_APPEARANCE",
                          "-material":
                            { "Material":
                              {
                                "@diffuseColor":[0.9,0.5,0.1],
                                "@emissiveColor":[0.2,0.1,0.05]
                              }
                            }
                        }
                      }
                  }
                },
                { "SphereSensor":
                  {
                    "@DEF":"FIRST_SPHERESENSOR",
                    "@description":"click and drag to rotate"
                  }
                }
              ]
            }
          },
          {
            "#comment":"Note that we are connecting a rotation output to a rotation input in each of these ROUTEs."
          },
          { "ROUTE":
            {
              "@fromField":"rotation_changed",
              "@fromNode":"FIRST_SPHERESENSOR",
              "@toField":"set_rotation",
              "@toNode":"FIRST_TRANSFORM"
            }
          },
          { "Transform":
            {
              "@DEF":"SECOND_TRANSFORM",
              "@translation":[2,0,0],
              "-children":[
                { "Shape":
                  {
                    "-geometry":
                      { "Cone":
                        {
                        }
                      },
                    "-appearance":
                      { "Appearance":
                        {
                          "@USE":"FIRST_APPEARANCE"
                        }
                      }
                  }
                },
                { "SphereSensor":
                  {
                    "@DEF":"SECOND_SPHERESENSOR",
                    "@description":"click and drag to rotate"
                  }
                }
              ]
            }
          },
          { "ROUTE":
            {
              "@fromField":"rotation_changed",
              "@fromNode":"SECOND_SPHERESENSOR",
              "@toField":"set_rotation",
              "@toNode":"SECOND_TRANSFORM"
            }
          }
        ]
    }
  }
