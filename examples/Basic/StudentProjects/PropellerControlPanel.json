                    "#sourceText":[
"ecmascript:",
"",
"function switchPosition(pos)",
"{",
"\tif (switchPosition.pos.y == 0){",
"\t\tfanCycleTime[0] = switchPosition.pos.y;",
"\t\tdisplayFanSpeed[0] ='OFF';",
"\t}",
"\telse{if (switchPosition.pos.y == 3){",
"\t\tfanCycleTime[0] = .01;",
"\t\tdisplayFanSpeed[0] ='MAX';",
"\t}",
"\telse {",
"\t\tfanCycleTime[0] = (6 - (switchPosition.pos.y*2));",
"\t\tdisplayFanSpeed[0] = (Math.round((1/fanCycleTime[0])*6000)/100) + ' RPM';",
"\t}}",
"}"
]
