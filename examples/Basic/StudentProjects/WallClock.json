"function set_second () {",
" var today = new Date();",
" var seconds = today.getSeconds();",
" var minutes =  -(Math.PI/30)*today.getMinutes();",
" var hours = -(Math.PI/6)*today.getHours();",
" second_changed[3] = -(Math.PI/30)*seconds ;",
" minute_changed[3]= minutes -(Math.PI/30)*seconds/60; ",
