{ "X3D": {
    "encoding":"UTF-8",
    "@profile":"Immersive",
    "@version":"3.3",
    "@xsd:noNamespaceSchemaLocation":"http://www.web3d.org/specifications/x3d-3.3.xsd",
    "JSON schema":"http://www.web3d.org/specifications/x3d-3.3-JSONSchema.json",
    "head": {
        "meta": [
          {
            "@name":"title",
            "@content":"ECMAScriptSaiTouchSensorIsOverEvent.x3d"
          },
          {
            "@name":"description",
            "@content":"Java Scene Authoring Interface (JSAI) scene and source code example that demonstrate a scripted response to a TouchSensor isOver event by changing the color of a Box from blue to red."
          },
          {
            "@name":"creator",
            "@content":"X3D Working Group"
          },
          {
            "@name":"created",
            "@content":"23 February 2005"
          },
          {
            "@name":"modified",
            "@content":"26 November 2015"
          },
          {
            "@name":"reference",
            "@content":"http://www.web3d.org/files/specifications/19777-1/V3.3/Part1/X3D_ECMAScript.html"
          },
          {
            "@name":"specificationSection",
            "@content":"X3D language bindings, ISO/IEC 19777-1, Part 1: ECMAScript, B.2 touchSensor isOver event"
          },
          {
            "@name":"specificationUrl",
            "@content":"http://www.web3d.org/files/specifications/19777-1/V3.3/Part1/examples.html#TouchSensorIsOverEvent"
          },
          {
            "@name":"generator",
            "@content":"X3D-Edit 3.3, https://savage.nps.edu/X3D-Edit"
          },
          {
            "@name":"identifier",
            "@content":"http://www.web3d.org/x3d/content/examples/Basic/X3dSpecifications/ECMAScriptSaiTouchSensorIsOverEvent.x3d"
          },
          {
            "@name":"license",
            "@content":"../license.html"
          },
          {
            "@name":"translated",
            "@content":"19 March 2017"
          },
          {
            "@name":"generator",
            "@content":"X3dToJson.xslt, http://www.web3d.org/x3d/stylesheets/X3dToJson.html"
          },
          {
            "@name":"warning",
            "@content":"An experimental version of X3D JSON encoding is used for this scene.  Status online at http://www.web3d.org/wiki/index.php/X3D_JSON_Encoding"
          }
        ]
    },
    "Scene": {
        "-children":[
          { "Group":
            {
              "-children":[
                { "Shape":
                  {
                    "-appearance":
                      { "Appearance":
                        {
                          "-material":
                            { "Material":
                              {
                                "@DEF":"MAT",
                                "@diffuseColor":[0,0,1]
                              }
                            }
                        }
                      },
                    "-geometry":
                      { "Box":
                        {
                        }
                      }
                  }
                },
                { "TouchSensor":
                  {
                    "@DEF":"TS"
                  }
                }
              ]
            }
          },
          { "Script":
            {
              "@DEF":"SC",
              "field": [
                {
                  "@name":"isOver",
                  "@accessType":"inputOnly",
                  "@type":"SFBool"
                },
                {
                  "@name":"diffuseColor_changed",
                  "@accessType":"outputOnly",
                  "@type":"SFColor"
                }
              ],
              "#sourceText":[
"ecmascript:",
"function initialize() { }",
"function isOver(val) {",
"    if (val == true) {",
"        diffuseColor_changed = new SFColor(1,0,0);",
"    } else {",
"        diffuseColor_changed = new SFColor(0,0,1);",
"    }",
"}"
]
            }
          },
          { "ROUTE":
            {
              "@fromField":"isOver",
              "@fromNode":"TS",
              "@toField":"isOver",
              "@toNode":"SC"
            }
          },
          { "ROUTE":
            {
              "@fromField":"diffuseColor_changed",
              "@fromNode":"SC",
              "@toField":"set_diffuseColor",
              "@toNode":"MAT"
            }
          }
        ]
    }
  }
}