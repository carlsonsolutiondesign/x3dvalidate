                      {
                        "@fromField":"startEngine",
                        "@fromNode":"RotorScript",
                        "@toField":"startTime",
                        "@toNode":"Top"
                      }
                    },
                    { "ROUTE":
                      {
                        "@fromField":"stopEngine",
                        "@fromNode":"RotorScript",
                        "@toField":"stopTime",
                        "@toNode":"Top"
                      }
                    }
                  ]
