                            "@accessType":"initializeOnly",
                            "@type":"SFBool",
                            "@value":false
                          }
                        ],
                        "#sourceText":[
"ecmascript:",
"",
"function startOrStopEngine(value)",
"{",
"\t// start or stop engine:",
"\tif (!engineStarted)",
"\t{",
"\t\tstartEngine = value;",
"\t\tengineStarted = true;",
"\t}",
"\telse",
"\t{",
"\t\tstopEngine = value;",
"\t\tengineStarted = false;",
"\t}",
"}"
]
