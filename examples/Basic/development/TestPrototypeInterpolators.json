                              "@key":[0,0.5,1],
                              "@keyValue":[0,1,0,0,0.7071,0.7071,0,3.1416,0,1,0,0],
                              "IS": {
                                  "connect": [
                                    {
                                      "@nodeField":"set_fraction",
                                      "@protoField":"set_fraction"
                                    },
                                    {
                                      "@nodeField":"value_changed",
                                      "@protoField":"orientation_changed"
                                    }
                                  ]
                              }
                            }
                          }
                        ]
                      }
