    "Scene": {
        "-children":[
          { "ExternProtoDeclare":
            {
              "@name":"MyNodeAppearance",
              "@url":["http://www.web3d.org/somewhere/MyNodePROTOs.wrl#MyNodeAppearance"]
            }
          },
          { "ExternProtoDeclare":
