{ "X3D": {
    "encoding":"UTF-8",
    "@profile":"Immersive",
    "@version":"3.0",
    "@xsd:noNamespaceSchemaLocation":"http://www.web3d.org/specifications/x3d-3.0.xsd",
    "JSON schema":"http://www.web3d.org/specifications/x3d-3.3-JSONSchema.json",
    "head": {
        "meta": [
          {
            "@name":"title",
            "@content":"LODlevelChangedExample.x3d"
          },
          {
            "@name":"description",
            "@content":"Example use of (proposed) LOD level_changed outputOnly field to trigger loading of Inline nodes in subsequent levels."
          },
          {
            "@name":"creator",
            "@content":"Don Brutzman"
          },
          {
            "@name":"motivation",
            "@content":"Chris Thorne"
          },
          {
            "@name":"created",
            "@content":"26 May 2004"
          },
          {
            "@name":"modified",
            "@content":"12 January 2014"
          },
          {
            "@name":"warning",
            "@content":"under development, evaluating a proposed specification change"
          },
          {
            "@name":"identifier",
            "@content":"http://www.web3d.org/x3d/content/examples/Basic/development/LODlevelChangedExample.x3d"
          },
          {
            "@name":"generator",
            "@content":"X3D-Edit 3.3, https://savage.nps.edu/X3D-Edit"
          },
          {
            "@name":"license",
            "@content":"../license.html"
          },
          {
            "@name":"translated",
            "@content":"19 March 2017"
          },
          {
            "@name":"generator",
            "@content":"X3dToJson.xslt, http://www.web3d.org/x3d/stylesheets/X3dToJson.html"
          },
          {
            "@name":"warning",
            "@content":"An experimental version of X3D JSON encoding is used for this scene.  Status online at http://www.web3d.org/wiki/index.php/X3D_JSON_Encoding"
          }
        ]
    },
    "Scene": {
        "-children":[
          { "LOD":
            {
              "@DEF":"activeLOD",
              "@range":[6,10],
              "-children":[
                { "Shape":
                  {
                    "@DEF":"ShapeLevel0",
                    "-geometry":
                      { "Text":
                        {
                          "@string":["developmental test scene","LODlevelChangedExample"],
                          "-fontStyle":
                            { "FontStyle":
                              {
                                "@justify":["MIDDLE","MIDDLE"]
                              }
                            }
                        }
                      }
                  }
                },
                { "Inline":
                  {
                    "@DEF":"InlineLevel1",
                    "@load":false
                  }
                },
                { "Inline":
                  {
                    "@DEF":"InlineLevel2",
                    "@load":false
                  }
                },
                {
                  "#comment":"etc."
                }
              ]
            }
          },
          { "Group":
            {
              "@DEF":"PreloadActivationGroup",
              "-children":[
                { "Script":
                  {
                    "@DEF":"PreloadLevelsScript",
                    "field": [
                      {
                        "@name":"level_changed",
                        "@accessType":"inputOnly",
                        "@type":"SFInt32"
                      },
                      {
                        "@name":"activateLevel1",
                        "@accessType":"outputOnly",
                        "@type":"SFBool"
                      },
                      {
                        "@name":"activateLevel2",
                        "@accessType":"outputOnly",
                        "@type":"SFBool"
                      }
                    ],
                    "#sourceText":[
"ecmascript:",
"",
"function level_changed (value, timestamp)",
"{",
"\tBrowser.print ('level=' + value);",
"\tif      (value == 1) activateLevel1 = true;",
"\telse if (value == 2) activateLevel2 = true;",
"\t// etc.",
"}"
]
                  }
                },
                { "ROUTE":
                  {
                    "@fromField":"level_changed",
                    "@fromNode":"activeLOD",
                    "@toField":"level_changed",
                    "@toNode":"PreloadLevelsScript"
                  }
                },
                { "ROUTE":
                  {
                    "@fromField":"activateLevel1",
                    "@fromNode":"PreloadLevelsScript",
                    "@toField":"load",
                    "@toNode":"InlineLevel1"
                  }
                },
                { "ROUTE":
                  {
                    "@fromField":"activateLevel2",
                    "@fromNode":"PreloadLevelsScript",
                    "@toField":"load",
                    "@toNode":"InlineLevel2"
                  }
                }
              ]
            }
          },
          { "Group":
            {
              "@DEF":"ViewpointGroup",
              "-children":[
                { "Viewpoint":
                  {
                    "@description":"level 0",
                    "@position":[0,0,4]
                  }
                },
                { "Viewpoint":
                  {
                    "@description":"level 1",
                    "@position":[0,0,8]
                  }
                },
                { "Viewpoint":
                  {
                    "@description":"level 2",
                    "@position":[0,0,12]
                  }
                }
              ]
            }
          }
        ]
    }
  }
}