"ecmascript:",
"",
"function initialize ()",
"{",
"\tevaluatedResult = new MFString (",
"\t\t'Click for partial Scene node test:',",
"\t\t' ', ' ', ' ', ' ', ' ', ' ', ' ');",
"}",
"function currentFrameRate (inputValue, timeStamp)",
"{",
"\tevaluatedResult [2] = 'currentFrameRate=' + inputValue;",
"}",
"function currentSpeed (inputValue, timeStamp)",
"{",
"\tevaluatedResult [3] = 'currentSpeed=' + inputValue;",
"}",
"function version (inputValue, timeStamp)",
"{",
"\tevaluatedResult [4] = 'version=' + inputValue;",
"}",
"function name (inputValue, timeStamp)",
"{",
"\tevaluatedResult [5] = 'name=' + inputValue;",
"}",
"function worldUrl (inputValue, timeStamp)",
"{",
"\tevaluatedResult [6] = 'worldUrl=';",
"\tevaluatedResult [7] = inputValue;",
"}"
]
