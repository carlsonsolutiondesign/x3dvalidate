"ecmascript:",
"",
"function evaluate (inputValue, timeStamp)",
"{",
"\tif (inputValue)  // Boolean",
"\t{",
"\t\tcurrentFrameRate = Browser.getCurrentFrameRate ();",
"\t\tcurrentSpeed = Browser.getCurrentSpeed ();",
"\t\tversion = Browser.getVersion ();",
"\t\tname = Browser.getName ();",
"\t\tworldUrl = Browser.getWorldURL ();",
"",
"\t\tBrowser.print ('currentFrameRate = ' + currentFrameRate);",
"\t\tBrowser.print ('currentSpeed = ' + currentSpeed);",
"\t\tBrowser.print ('version = ' + version);",
"\t\tBrowser.print ('name = ' + name);",
"\t\tBrowser.print ('worldUrl = ' + worldUrl);",
"\t}",
"}",
"",
"function createVrmlFromString (inputValue, timeStamp)",
"{",
"\t// note VRML 97 method name didn't follow capitalization convention",
"\tcreatedVrmlNodes = Browser.createVrmlFromString (inputValue);",
"}",
"",
"function createVrmlFromUrl (inputValue, timeStamp)",
"{",
"\t// note VRML 97 method name didn't follow capitalization convention",
"\tcreatedVrmlNodes = Browser.createVrmlFromURL (inputValue);",
"}",
"",
"function description (inputValue, timeStamp)",
"{",
"\tBrowser.print ('description = ' + inputValue);",
"\tBrowser.setDescription (inputValue);",
"}",
"",
"function replaceWorld (inputValue, timeStamp)",
"{",
"\tBrowser.replaceWorld (inputValue);",
"}",
"",
"function addRoute (inputValue, timeStamp)",
"{",
"\tif (inputValue)  // Boolean",
"\t{",
"\t\tBrowser.addRoute (fromNode, fromEventOut, toNode, toEventIn);",
"\t}",
"}",
"",
"function deleteRoute (inputValue, timeStamp)",
"{",
"\tif (inputValue)  // Boolean",
"\t{",
"\t\tBrowser.deleteRoute (fromNode, fromEventOut, toNode, toEventIn);",
"\t}",
"}",
"",
"function loadURL (inputValue, timeStamp)",
"{",
"\tBrowser.print ('loadURL = ' + inputValue);",
"\tBrowser.print ('loadUrlParameter = ' + loadUrlParameter);",
"\tBrowser.loadURL (inputValue, loadUrlParameter);",
"}",
"function createVrmlFromURL (SFStringValue, timestamp)",
"{",
"",
"}",
"",
"function loadUrl (SFStringValue, timestamp)",
"{",
"",
"}",
"",
