          {
            "#comment":"Proximity activation triggers switch in NavigationInfo mode between EXAMINE and WALK - pretty cool!"
          },
          { "NavigationInfo":
            {
              "@DEF":"NAVIGATION",
              "@speed":20,
              "@type":["WALK","EXAMINE","ANY"]
            }
          },
          { "Script":
            {
              "@DEF":"NAVIGATION_MODE",
              "field": [
                {
                  "@name":"inProximity",
                  "@accessType":"inputOnly",
                  "@type":"SFBool"
                },
                {
                  "@name":"type",
                  "@accessType":"outputOnly",
                  "@type":"MFString"
                }
              ],
              "#sourceText":[
"ecmascript:",
"",
"function inProximity ( value, eventTime )",
"{",
" if ( value == true )",
" {",
"\ttype[0] = 'EXAMINE';",
"\ttype[1] = 'WALK';",
"\ttype[2] = 'ANY';",
" }",
" else",
" {",
"\ttype[0] = 'WALK';",
"\ttype[1] = 'EXAMINE';",
"\ttype[2] = 'ANY';",
" }",
" // Browser.print ('inProximity=' + value + ', type=' + type);",
"}"
]
            }
          },
          { "ROUTE":
            {
              "@fromField":"proximityIsActive",
              "@fromNode":"BLUE_SPHERICAL_MINE",
              "@toField":"inProximity",
              "@toNode":"NAVIGATION_MODE"
            }
          },
          { "ROUTE":
            {
              "@fromField":"proximityIsActive",
              "@fromNode":"GREEN_SPHERICAL_MINE",
              "@toField":"inProximity",
              "@toNode":"NAVIGATION_MODE"
            }
          },
          { "ROUTE":
            {
              "@fromField":"type",
              "@fromNode":"NAVIGATION_MODE",
              "@toField":"set_type",
              "@toNode":"NAVIGATION"
            }
