"ecmascript:",
"",
"// only react to 'true' TouchSensor events, cut computation of normals etc. in half",
"",
"function touched80m (value, ts)",
"{",
"  if (value == true)",
"  {",
"\tselection = 0;",
"\tcolor80m  = new SFColor (0, .8, 0);",
"\tcolor40m  = new SFColor (.8, 0, 0);",
"\tcolor20m  = new SFColor (.8, 0, 0);",
"\tcolor10m  = new SFColor (.8, 0, 0);",
"\tcolor05m  = new SFColor (.8, 0, 0);",
"  }",
"}",
"function touched40m (value, ts)",
"{",
"  if (value == true)",
"  {",
"\tselection = 1;",
"\tcolor80m  = new SFColor (.8, 0, 0);",
"\tcolor40m  = new SFColor (0, .8, 0);",
"\tcolor20m  = new SFColor (.8, 0, 0);",
"\tcolor10m  = new SFColor (.8, 0, 0);",
"\tcolor05m  = new SFColor (.8, 0, 0);",
"  }",
"}",
"function touched20m (value, ts)",
"{",
"  if (value == true)",
"  {",
"\tselection = 2;",
"\tcolor80m  = new SFColor (.8, 0, 0);",
"\tcolor40m  = new SFColor (.8, 0, 0);",
"\tcolor20m  = new SFColor (0, .8, 0);",
"\tcolor10m  = new SFColor (.8, 0, 0);",
"\tcolor05m  = new SFColor (.8, 0, 0);",
"  }",
"}",
"function touched10m (value, ts)",
"{",
"  if (value == true)",
"  {",
"\tselection = 3;",
"\tcolor80m  = new SFColor (.8, 0, 0);",
"\tcolor40m  = new SFColor (.8, 0, 0);",
"\tcolor20m  = new SFColor (.8, 0, 0);",
"\tcolor10m  = new SFColor (0, .8, 0);",
"\tcolor05m  = new SFColor (.8, 0, 0);",
"  }",
"}",
"function touched05m (value, ts)",
"{",
"  Browser.print ('05m dataset not available...'); // goes to VRML console only, not scene",
"}"
]
