    "Scene": {
        "-children":[
          { "ProtoDeclare":
            {
              "@name":"DoubleClickTouchSensor",
              "@appinfo":"DoubleClickTouchSensor functionality activates when user double clicks (or double selects) within maxDelayInterval",
              "ProtoInterface": {
                  "field": [
                    {
                      "@name":"description",
                      "@accessType":"inputOutput",
                      "@appinfo":"describe the purpose of this sensor",
                      "@type":"SFString"
                    },
                    {
                      "@name":"maxDelayInterval",
                      "@accessType":"initializeOnly",
                      "@appinfo":"seconds",
                      "@type":"SFTime",
                      "@value":0.5
                    },
                    {
                      "@name":"enabled",
                      "@accessType":"inputOutput",
                      "@type":"SFBool",
                      "@value":true
                    },
                    {
                      "@name":"isActive",
                      "@accessType":"outputOnly",
                      "@type":"SFBool"
                    },
                    {
                      "@name":"isOver",
                      "@accessType":"outputOnly",
                      "@type":"SFBool"
                    },
                    {
                      "@name":"touchTime",
                      "@accessType":"outputOnly",
                      "@appinfo":"seconds",
                      "@type":"SFTime"
                    },
                    {
                      "@name":"hitPoint_changed",
                      "@accessType":"outputOnly",
                      "@type":"SFVec3f"
                    },
                    {
                      "@name":"hitNormal_changed",
                      "@accessType":"outputOnly",
                      "@type":"SFVec3f"
                    },
                    {
                      "@name":"hitTexCoord_changed",
                      "@accessType":"outputOnly",
                      "@type":"SFVec2f"
                    },
                    {
                      "@name":"metadata",
                      "@accessType":"initializeOnly",
                      "@type":"SFNode",
                      "-children":[
                        {
                          "#comment":"initialization node (if any) goes here"
                        }
                      ]
                    },
                    {
                      "@name":"traceEnabled",
                      "@accessType":"initializeOnly",
                      "@type":"SFBool",
                      "@value":false
                    }
                  ]
              },
              "ProtoBody": {
                  "-children":[
                    { "TouchSensor":
                      {
                        "@DEF":"TouchSensorNode",
                        "IS": {
                            "connect": [
                              {
                                "@nodeField":"description",
                                "@protoField":"description"
                              },
                              {
                                "@nodeField":"enabled",
                                "@protoField":"enabled"
                              },
                              {
                                "@nodeField":"isOver",
                                "@protoField":"isOver"
                              }
                            ]
                        }
                      }
                    },
                    {
                      "#comment":"Only first node inside a ProtoBody is rendered. TouchSensor must remain outside Group to be externally active."
                    },
                    { "Group":
                      {
                        "-children":[
                          { "Script":
                            {
                              "@DEF":"DoubleClickTouchScript",
                              "@directOutput":true,
                              "field": [
                                {
                                  "@name":"description",
                                  "@accessType":"inputOutput",
                                  "@type":"SFString"
                                },
                                {
                                  "@name":"checkClicks",
                                  "@accessType":"inputOnly",
                                  "@type":"SFBool"
                                },
                                {
                                  "@name":"maxDelayInterval",
                                  "@accessType":"initializeOnly",
                                  "@appinfo":"seconds",
                                  "@type":"SFTime"
                                },
                                {
                                  "@name":"priorTouchTime",
                                  "@accessType":"initializeOnly",
                                  "@type":"SFTime",
                                  "@value":-1
                                },
                                {
                                  "@name":"currentTouchTime",
                                  "@accessType":"initializeOnly",
                                  "@type":"SFTime",
                                  "@value":-1
                                },
                                {
                                  "@name":"touchSensor",
                                  "@accessType":"initializeOnly",
                                  "@appinfo":"permits controlled access to various TouchSensorNode fields",
                                  "@type":"SFNode",
                                  "-children":[
                                    { "TouchSensor":
                                      {
                                        "@USE":"TouchSensorNode"
                                      }
                                    }
                                  ]
                                },
                                {
                                  "@name":"isActive",
                                  "@accessType":"outputOnly",
                                  "@type":"SFBool"
                                },
                                {
                                  "@name":"touchTime",
                                  "@accessType":"outputOnly",
                                  "@appinfo":"seconds",
                                  "@type":"SFTime"
                                },
                                {
                                  "@name":"hitPoint_changed",
                                  "@accessType":"outputOnly",
                                  "@type":"SFVec3f"
                                },
                                {
                                  "@name":"hitNormal_changed",
                                  "@accessType":"outputOnly",
                                  "@type":"SFVec3f"
                                },
                                {
                                  "@name":"hitTexCoord_changed",
                                  "@accessType":"outputOnly",
                                  "@type":"SFVec2f"
                                },
                                {
                                  "@name":"traceEnabled",
                                  "@accessType":"initializeOnly",
                                  "@type":"SFBool"
                                }
                              ],
                              "IS": {
                                  "connect": [
                                    {
                                      "@nodeField":"description",
                                      "@protoField":"description"
                                    },
                                    {
                                      "@nodeField":"maxDelayInterval",
                                      "@protoField":"maxDelayInterval"
                                    },
                                    {
                                      "@nodeField":"isActive",
                                      "@protoField":"isActive"
                                    },
                                    {
                                      "@nodeField":"touchTime",
                                      "@protoField":"touchTime"
                                    },
                                    {
                                      "@nodeField":"hitPoint_changed",
                                      "@protoField":"hitPoint_changed"
                                    },
                                    {
                                      "@nodeField":"hitNormal_changed",
                                      "@protoField":"hitNormal_changed"
                                    },
                                    {
                                      "@nodeField":"hitTexCoord_changed",
                                      "@protoField":"hitTexCoord_changed"
                                    },
                                    {
                                      "@nodeField":"traceEnabled",
                                      "@protoField":"traceEnabled"
                                    }
                                  ]
                              },
                              "#sourceText":[
"ecmascript:",
"",
"//  inputOnly events are handled by functions,",
"//  initializeOnly fields are variable objects,",
"//  outputOnly events are sent by setting values",
"",
"function initialize ()",
"{",
"\ttracePrint ('initialize (); enabled=' + touchSensor.enabled);",
"}",
"",
"// trigger on isActive to filter touchTime events when unclicking",
"",
"function checkClicks (value, timestamp)  // activated from TouchSensorNode.isActive",
"{",
"\tif (value == false) // doubleClick isActive=false events",
"\t{",
"\t\tif (isActive == true) // need to unclick",
"\t\t{",
"\t\t\tisActive  = false;\t// send output events",
"\t\t\ttouchTime = timestamp;",
"\t\t\ttracePrint ('DoubleClick isActive=false;');",
"\t\t}",
"\t\treturn;",
"\t}",
"\tpriorTouchTime = currentTouchTime;",
"\tcurrentTouchTime = timestamp;",
"\tcomputedDelay = (currentTouchTime - priorTouchTime);",
"",
"\tif      (priorTouchTime == -1)",
"\t{",
"\t\ttracePrint ('priorTouchTime == -1, return;');",
"\t\treturn;",
"\t}",
"\telse if (computedDelay <= maxDelayInterval)",
"\t{",
"\t\ttracePrint ('computedDelay=' + computedDelay + ', DoubleClick isActive=true;');",
"",
"\t\tisActive  = true;\t// send output events",
"\t\ttouchTime = timestamp;",
"\t\thitPoint_changed    = touchSensor.hitPoint_changed;",
"\t\thitNormal_changed   = touchSensor.hitNormal_changed;",
"\t\thitTexCoord_changed = touchSensor.hitTexCoord_changed;",
"\t}",
"\telse",
"\t{",
"\t\ttracePrint ('computedDelay=' + computedDelay + ', single click');",
"\t\treturn;",
"\t}",
"",
"}",
"function set_description (newDescription)",
"{",
"    description = newDescription;",
"}",
"function tracePrint (outputString)",
"{",
"\tif (traceEnabled) Browser.print ('[DoubleClickTouchSensor] ' + outputString);",
"}"
]
                            }
                          },
                          { "ROUTE":
                            {
                              "@fromField":"isActive",
                              "@fromNode":"TouchSensorNode",
                              "@toField":"checkClicks",
                              "@toNode":"DoubleClickTouchScript"
                            }
                          },
                          { "Collision":
                            {
                              "@DEF":"MetadataHolder",
                              "@enabled":false,
                              "IS": {
                                  "connect": [
                                    {
                                      "@nodeField":"proxy",
                                      "@protoField":"metadata"
                                    }
                                  ]
                              }
                            }
                          }
                        ]
                      }
                    }
                  ]
              }
            }
          },
