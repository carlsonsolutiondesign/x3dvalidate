                },
                {
                  "@name":"Light",
                  "@accessType":"initializeOnly",
                  "@type":"SFBool",
                  "@value":true
                }
              ],
              "#sourceText":[
"ecmascript:",
"",
"      function Position (value) {",
"\tmsg = 'Control Panel';",
"\tif (value[1] < .5) {",
"\t  if (value[0] > .5) {",
"\t    Light = false;",
"\t    msg = 'Headlight Off';",
"\t   } else {",
"\t    Light = true;",
"\t    msg = 'Headlight On';",
"\t  }",
"\t}",
"\t// Browser.setDescription (msg); // TODO figure this out, see Table 7.2 in Ecmascript SAI",
"      }",
"",
"      function Touch (value) {",
"\tHeadlight = Light;",
"      }"
]
            }
          },
          { "ROUTE":
            {
              "@fromField":"hitTexCoord_changed",
