                  "@name":"Headlight",
                  "@accessType":"outputOnly",
                  "@type":"SFBool"
                },
                {
                  "@name":"Position",
                  "@accessType":"inputOnly",
                  "@type":"SFVec2f"
                },
                {
                  "@name":"Touch",
                  "@accessType":"inputOnly",
