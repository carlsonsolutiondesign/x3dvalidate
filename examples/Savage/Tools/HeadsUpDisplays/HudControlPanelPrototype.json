    "head": {
        "meta": [
          {
            "@name":"title",
            "@content":"HudControlPanelPrototype.x3d"
          },
          {
            "@name":"description",
            "@content":"Prototype for a \"heads-up\" control panel. This node defines an object that maintains its orientation to the current viewpoint. Also present is a border that allows the panel to be dragged anywhere on the screen. All of the touchSensor events relating to texture maps are available as output events from this node."
          },
          {
            "@name":"creator",
            "@content":"Leonard Daly"
          },
          {
            "@name":"translator",
            "@content":"Dan Kucik"
          },
          {
            "@name":"created",
            "@content":"1999"
          },
          {
            "@name":"translated",
            "@content":"8 June 2003"
          },
          {
            "@name":"modified",
            "@content":"14 January 2014"
          },
          {
            "@name":"error",
            "@content":"HUD does not sit still when navigating"
          },
          {
            "@name":"reference",
            "@content":"http://www.realism.com/Web3D/Examples/examples.cgi?ID=5"
          },
          {
            "@name":"rights",
            "@content":"This code may be reused in part or total. Please include this paragraph when it is used. Copyright 1999, Leonard Daly #****************"
          },
          {
            "@name":"subject",
            "@content":"Heads-up display example"
          },
          {
            "@name":"reference",
            "@content":"Original VRML File: http://www.realism.com/vrml/Example/HUD/HUD_proto.wrl"
          },
          {
            "@name":"identifier",
            "@content":"https://savage.nps.edu/Savage/Tools/HeadsUpDisplays/HudControlPanelPrototype.x3d"
          },
          {
            "@name":"generator",
            "@content":"Vrml97ToX3dNist, http://ovrt.nist.gov/v2_x3d.html"
          },
          {
            "@name":"generator",
            "@content":"X3D-Edit 3.2, https://savage.nps.edu/X3D-Edit"
          },
          {
            "@name":"license",
            "@content":"../../license.html"
          },
          {
            "@name":"translated",
            "@content":"19 March 2017"
          },
          {
            "@name":"generator",
            "@content":"X3dToJson.xslt, http://www.web3d.org/x3d/stylesheets/X3dToJson.html"
          },
          {
            "@name":"warning",
            "@content":"An experimental version of X3D JSON encoding is used for this scene.  Status online at http://www.web3d.org/wiki/index.php/X3D_JSON_Encoding"
          }
        ]
    },
    "Scene": {
        "-children":[
          { "ProtoDeclare":
            {
              "@name":"HudControlPanel",
              "@appinfo":"Heads Up Display (HUD) Control Panel",
              "ProtoInterface": {
                  "field": [
                    {
                      "@name":"translation",
                      "@accessType":"initializeOnly",
                      "@appinfo":"SFVec3f Position of the panel relative to the viewpoint. The X and Y components can be changed by dragging the window around.",
                      "@type":"SFVec3f",
                      "@value":[0.0,0.0,-10.0]
                    },
                    {
                      "@name":"orientation",
                      "@accessType":"inputOutput",
                      "@appinfo":"SFRotation This field should be the object of a ROUTE statement from the orientation_changed event from a ProximitySensor. For some reason the ProximitySensor does not correctly report the current viewpoint when the sensor is located inside of the PROTO.",
                      "@type":"SFRotation",
                      "@value":[0.0,1.0,0.0,0.0]
                    },
                    {
                      "@name":"isOver",
                      "@accessType":"outputOnly",
                      "@appinfo":"Out - SFBool event is generated when the cursor is over the texture mapped region of the control panel. Set to false when the cursor leaves the texture mapped region.",
                      "@type":"SFBool"
                    },
                    {
                      "@name":"hitTexCoord_changed",
                      "@accessType":"outputOnly",
                      "@appinfo":"Out - SFVec2f The X/Y normalized coordinates (S and T) of the cursor location when isOver is true.",
                      "@type":"SFVec2f"
                    },
                    {
                      "@name":"borderColor",
                      "@accessType":"inputOutput",
                      "@appinfo":"SFColor The color of the panel border. This is the region that can be used to drag the panel around the screen.",
                      "@type":"SFColor",
                      "@value":[0.6,0.6,0.6]
                    },
                    {
                      "@name":"scale",
                      "@accessType":"initializeOnly",
                      "@appinfo":"SFVec3f Size of the panel. The texture mapped area (see textureMap) is 95% in X and 90% in Y of this size.",
                      "@type":"SFVec3f",
                      "@value":[2.5,1.5,0.05]
                    },
                    {
                      "@name":"position",
                      "@accessType":"inputOutput",
                      "@appinfo":"SFVec3f This field should be the object of a ROUTE statement from the position_changed event from a ProximitySensor. See orientation for more details.",
                      "@type":"SFVec3f",
                      "@value":[0.0,0.0,0.0]
                    },
                    {
                      "@name":"textureMap",
                      "@accessType":"inputOutput",
                      "@appinfo":"MFString The texture map URLs. This is specified in the same manner as the URL field in ImageTexture. The first one that can be loaded is the that is displayed. If this field is not specified then this region of the object is painted white.",
                      "@type":"MFString"
                    },
                    {
                      "@name":"touchTime",
                      "@accessType":"outputOnly",
                      "@appinfo":"Out - SFTime The time when a mouse button is released provided that the just prior to release isOver and isActive are true.",
                      "@type":"SFTime"
                    },
                    {
                      "@name":"isActive",
                      "@accessType":"outputOnly",
                      "@appinfo":"Out - SFBool A true event is generated when the mouse button is depressed and the cursor is over the texture mapped region. A false event is generated when the button is released or when isOver becomes false.",
                      "@type":"SFBool"
                    }
                  ]
              },
              "ProtoBody": {
                  "-children":[
                    { "Transform":
                      {
                        "@DEF":"Main",
                        "IS": {
                            "connect": [
                              {
                                "@nodeField":"translation",
                                "@protoField":"position"
                              },
                              {
                                "@nodeField":"rotation",
                                "@protoField":"orientation"
                              }
                            ]
                        },
                        "-children":[
                          { "Transform":
                            {
                              "@DEF":"CtlPnlOffset",
                              "IS": {
                                  "connect": [
                                    {
                                      "@nodeField":"translation",
                                      "@protoField":"translation"
                                    },
                                    {
                                      "@nodeField":"scale",
                                      "@protoField":"scale"
                                    }
                                  ]
                              },
                              "-children":[
                                { "DirectionalLight":
                                  {
                                  }
                                },
                                { "PlaneSensor":
                                  {
                                    "@DEF":"MovePanel",
                                    "@description":"Select item"
                                  }
                                },
                                { "Shape":
                                  {
                                    "-appearance":
                                      { "Appearance":
                                        {
                                          "-material":
                                            { "Material":
                                              {
                                                "@DEF":"_IS_0",
                                                "IS": {
                                                    "connect": [
                                                      {
                                                        "@nodeField":"diffuseColor",
                                                        "@protoField":"borderColor"
                                                      }
                                                    ]
                                                }
                                              }
                                            }
                                        }
                                      },
                                    "-geometry":
                                      { "IndexedFaceSet":
                                        {
                                          "@DEF":"Panel",
                                          "@coordIndex":[0,1,2,3,-1],
                                          "-coord":
                                            { "Coordinate":
                                              {
                                                "@point":[1.0,1.0,0.0,-1.0,1.0,0.0,-1.0,-1.0,0.0,1.0,-1.0,0.0]
                                              }
                                            }
                                        }
                                      }
                                  }
                                },
                                { "Transform":
                                  {
                                    "@scale":[0.95,0.9,1.0],
                                    "@translation":[0.0,0.0,0.01],
                                    "-children":[
                                      { "TouchSensor":
                                        {
                                          "@description":"Select item",
                                          "IS": {
                                              "connect": [
                                                {
                                                  "@nodeField":"isOver",
                                                  "@protoField":"isOver"
                                                },
                                                {
                                                  "@nodeField":"isActive",
                                                  "@protoField":"isActive"
                                                },
                                                {
                                                  "@nodeField":"hitTexCoord_changed",
                                                  "@protoField":"hitTexCoord_changed"
                                                },
                                                {
                                                  "@nodeField":"touchTime",
                                                  "@protoField":"touchTime"
                                                }
                                              ]
                                          }
                                        }
                                      },
                                      { "Shape":
                                        {
                                          "-appearance":
                                            { "Appearance":
                                              {
                                                "-material":
                                                  { "Material":
                                                    {
                                                      "@diffuseColor":[1.0,1.0,1.0]
                                                    }
                                                  },
                                                "-texture":
                                                  { "ImageTexture":
                                                    {
                                                      "@DEF":"_IS_1",
                                                      "IS": {
                                                          "connect": [
                                                            {
                                                              "@nodeField":"url",
                                                              "@protoField":"textureMap"
                                                            }
                                                          ]
                                                      }
                                                    }
                                                  }
                                              }
                                            },
                                          "-geometry":
                                            { "IndexedFaceSet":
                                              {
                                                "@USE":"Panel"
                                              }
                                            }
                                        }
                                      }
                                    ]
                                  }
                                }
                              ]
                            }
                          }
                        ]
                      }
                    },
                    { "Script":
                      {
                        "@DEF":"MoveScript",
                        "field": [
                          {
                            "@name":"position",
                            "@accessType":"outputOnly",
                            "@type":"SFVec3f"
                          },
                          {
                            "@name":"move",
                            "@accessType":"inputOnly",
                            "@type":"SFVec3f"
                          },
                          {
                            "@name":"Iscale",
                            "@accessType":"initializeOnly",
                            "@type":"SFVec3f"
                          },
                          {
                            "@name":"Ioffset",
                            "@accessType":"initializeOnly",
                            "@type":"SFVec3f"
                          }
                        ],
                        "IS": {
                            "connect": [
                              {
                                "@nodeField":"Iscale",
                                "@protoField":"scale"
                              },
                              {
                                "@nodeField":"Ioffset",
                                "@protoField":"translation"
                              }
                            ]
                        },
                        "#sourceText":[
"ecmascript:",
"",
"      function move (value)",
"      {",
"\tposition = new SFVec3f (Iscale[0] * value[0] + Ioffset[0],",
"                                Iscale[1] * value[1] + Ioffset[1],",
"                                Ioffset[2]);",
"      }"
]
                      }
                    },
                    { "ROUTE":
                      {
                        "@fromField":"translation_changed",
                        "@fromNode":"MovePanel",
                        "@toField":"move",
                        "@toNode":"MoveScript"
                      }
                    },
                    { "ROUTE":
                      {
                        "@fromField":"position",
                        "@fromNode":"MoveScript",
                        "@toField":"translation",
                        "@toNode":"CtlPnlOffset"
                      }
                    }
                  ]
              }
            }
          },
          {
            "#comment":"===================="
          },
          { "Anchor":
            {
              "@description":"HudControlPanel Example",
              "@parameter":["target=_blank"],
              "@url":["HudControlPanelExample.x3d","https://savage.nps.edu/Savage/Tools/HeadsUpDisplays/HudControlPanelExample.x3d","HudControlPanelExample.wrl","https://savage.nps.edu/Savage/Tools/HeadsUpDisplays/HudControlPanelExample.wrl"],
              "-children":[
                { "Shape":
                  {
                    "-appearance":
                      { "Appearance":
                        {
                          "-material":
                            { "Material":
                              {
                                "@diffuseColor":[0,1,1],
                                "@emissiveColor":[0,1,1]
                              }
                            }
                        }
                      },
                    "-geometry":
                      { "Text":
                        {
                          "@string":["HudControlPanelPrototype","is a Prototype definition file.","","To see an example scene","using this new node","click this text and view","HudControlPanelExample"],
                          "-fontStyle":
                            { "FontStyle":
                              {
                                "@justify":["MIDDLE","MIDDLE"],
                                "@size":0.8
                              }
                            }
                        }
                      }
                  }
                }
              ]
            }
          }
        ]
    }
  }
}