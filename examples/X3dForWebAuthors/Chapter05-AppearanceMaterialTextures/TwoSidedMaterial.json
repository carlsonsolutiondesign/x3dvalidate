          }
        ]
    },
    "Scene": {
        "-children":[
          { "Background":
            {
              "@skyColor":[1,1,1]
            }
          },
          { "Viewpoint":
            {
              "@description":"Outside, front face material",
              "@position":[0,0,4]
            }
          },
          { "Viewpoint":
            {
              "@description":"Inside, back face material",
              "@position":[0,0,0]
            }
          },
          { "Viewpoint":
            {
              "@description":"Above, looking down on two shapes",
              "@orientation":[1,0,0,-1.57],
              "@position":[0,8,-2]
            }
          },
          { "NavigationInfo":
            {
              "@type":["WALK","ANY"]
            }
          },
          {
            "#comment":"Disable collision between viewpoint and sphere to facilitate viewing"
          },
          { "Collision":
            {
              "@enabled":false,
              "-children":[
                { "Shape":
                  {
                    "@DEF":"RedOutsideOrangeInside",
                    "-appearance":
                      { "Appearance":
                        {
                          "-material":
                            { "TwoSidedMaterial":
                              {
                                "@backDiffuseColor":[1,0.5,0],
                                "@backTransparency":0.5,
                                "@diffuseColor":[1,0,0],
                                "@separateBackColor":true,
                                "@transparency":0.5
                              }
                            }
                        }
                      },
                    "-children":[
                      {
                        "#comment":"Must have solid='false' to see inside of Sphere"
                      }
                    ],
                    "-geometry":
                      { "Sphere":
                        {
                          "@solid":false
                        }
                      }
                  }
                }
              ]
            }
          },
          { "Transform":
            {
              "@translation":[0,0,-4],
              "-children":[
                { "Shape":
                  {
                    "-geometry":
                      { "Box":
                        {
                          "@DEF":"BackgroundBox"
                        }
                      },
                    "-appearance":
                      { "Appearance":
                        {
                          "-material":
                            { "Material":
                              {
                                "@diffuseColor":[0,0.007843,0.839216]
                              }
                            }
                        }
                      }
                  }
                }
              ]
            }
          }
        ]
    }
