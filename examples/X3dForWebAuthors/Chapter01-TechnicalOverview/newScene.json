{ "X3D": {
    "encoding":"UTF-8",
    "@profile":"Immersive",
    "@version":"3.3",
    "@xsd:noNamespaceSchemaLocation":"http://www.web3d.org/specifications/x3d-3.3.xsd",
    "JSON schema":"http://www.web3d.org/specifications/x3d-3.3-JSONSchema.json",
    "head": {
        "-children":[
          {
            "#comment":"Authors need to replace asterisked values or delete unneeded meta tags as appropriate"
          },
          {
            "#comment":"Additional authoring resources for meta tags: http://www.dublincore.org/documents/dcmi-terms http://www.dublincore.org/documents/dces https://www.w3.org/TR/html5/document-metadata.html#the-meta-element http://www.w3.org/TR/html4/struct/global.html#h-7.4.4 http://vancouver-webpages.com/META http://vancouver-webpages.com/META/about-mk-metas2.html"
          },
          {
            "#comment":"Additional authoring resources for language codes: https://tools.ietf.org/html/bcp47 http://www.loc.gov/standards/iso639-2/langhome.html http://www.iana.org/numbers.html#index_L"
          }
        ],
        "meta": [
          {
            "@name":"title",
            "@content":"*enter FileNameWithNoAbbreviations.x3d here*"
          },
          {
            "@name":"description",
            "@content":"*enter description here, short-sentence summaries preferred*"
          },
          {
            "@name":"creator",
            "@content":"*enter name of original author here*"
          },
          {
            "@name":"translator",
            "@content":"*if manually translating VRML-to-X3D, enter name of person translating here*"
          },
          {
            "@name":"created",
            "@content":"*enter date of initial version here*"
          },
          {
            "@name":"translated",
            "@content":"*enter date of translation here*"
          },
          {
            "@name":"modified",
            "@content":"*enter date of latest revision here*"
          },
          {
            "@name":"version",
            "@content":"*enter version here, if any*"
          },
          {
            "@name":"reference",
            "@content":"*enter reference citation or relative/online url here*"
          },
          {
            "@name":"reference",
            "@content":"*enter additional url/bibliographic reference information here*"
          },
          {
            "@name":"requires",
            "@content":"*enter reference resource here if required to support function, delivery, or coherence of content*"
          },
          {
            "@name":"rights",
            "@content":"*enter copyright information here* Example: Copyright (c) Web3D Consortium Inc. 2006 to present"
          },
          {
            "@name":"drawing",
            "@content":"*enter drawing filename/url here*"
          },
          {
            "@name":"Image",
            "@content":"*enter image filename/url here*"
          },
          {
            "@name":"MovingImage",
            "@content":"*enter movie filename/url here*"
          },
          {
            "@name":"photo",
            "@content":"*enter photo filename/url here*"
          },
          {
            "@name":"subject",
            "@content":"*enter subject keywords here*"
          },
          {
            "@name":"accessRights",
            "@content":"*enter permission statements or url here*"
          },
          {
            "@name":"warning",
            "@content":"*insert any known warnings, bugs or errors here*"
          },
          {
            "@name":"identifier",
            "@content":"*enter online Uniform Resource Identifier (URI) or Uniform Resource Locator (URL) address for this file here*"
          },
          {
            "@name":"generator",
            "@content":"X3D-Edit 3.3, https://savage.nps.edu/X3D-Edit"
          },
          {
            "@name":"license",
            "@content":"../license.html"
          },
          {
            "@name":"translated",
            "@content":"19 March 2017"
          },
          {
            "@name":"generator",
            "@content":"X3dToJson.xslt, http://www.web3d.org/x3d/stylesheets/X3dToJson.html"
          },
          {
            "@name":"warning",
            "@content":"An experimental version of X3D JSON encoding is used for this scene.  Status online at http://www.web3d.org/wiki/index.php/X3D_JSON_Encoding"
          }
        ]
    },
    "Scene": {
        "-children":[
          {
            "#comment":"Scene graph nodes are added here"
          }
        ]
    }
  }
}