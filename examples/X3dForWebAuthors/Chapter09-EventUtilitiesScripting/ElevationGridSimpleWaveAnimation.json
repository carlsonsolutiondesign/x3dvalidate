    "JSON schema":"http://www.web3d.org/specifications/x3d-3.3-JSONSchema.json",
    "head": {
        "meta": [
          {
            "@name":"title",
            "@content":"ElevationGridSimpleWaveAnimation.x3d"
          },
          {
            "@name":"description",
            "@content":"Simple Javascript animation of ElevationGrid"
          },
          {
            "@name":"creator",
            "@content":"Don Brutzman"
          },
          {
            "@name":"created",
