    "Scene": {
        "-children":[
          {
            "#comment":"leftmost cylinder is vertical"
          },
          { "Transform":
            {
              "@translation":[-3.5,0,0],
              "-children":[
                { "Shape":
                  {
                    "@DEF":"TallCylinder",
                    "-geometry":
                      { "Cylinder":
