    "Scene": {
        "-children":[
          { "Background":
            {
              "@skyColor":[1,1,1]
            }
          },
          { "Viewpoint":
            {
              "@description":"Book View",
              "@orientation":[-0.668,-0.724,-0.174,0.7],
              "@position":[-1.93,1.78,3.28]
            }
          },
          { "Group":
            {
              "-children":[
                { "Shape":
                  {
                    "-appearance":
                      { "Appearance":
                        {
                          "-material":
                            { "Material":
                              {
                                "@diffuseColor":[1,0,0]
                              }
                            }
                        }
                      },
                    "-geometry":
                      { "Box":
                        {
                        }
                      }
                  }
                },
                { "Shape":
                  {
                    "-appearance":
                      { "Appearance":
                        {
                          "-material":
                            { "Material":
                              {
                                "@DEF":"Pulsar",
                                "@diffuseColor":[0,0,1]
                              }
                            }
                        }
                      },
                    "-geometry":
                      { "Sphere":
                        {
                          "@radius":1.4
                        }
                      }
                  }
                },
                {
                  "#comment":"here is a fun animation to change color of the Sphere, embedded inside the Group"
                },
                { "TimeSensor":
                  {
                    "@DEF":"Timer",
                    "@cycleInterval":6,
                    "@loop":true,
                    "@startTime":1
                  }
                },
                { "ColorInterpolator":
                  {
                    "@DEF":"Colorer",
                    "@key":[0,0.5,1],
                    "@keyValue":[0,0,1,0,1,0,0,0,1]
                  }
                },
                { "ROUTE":
                  {
                    "@fromField":"fraction_changed",
                    "@fromNode":"Timer",
                    "@toField":"set_fraction",
                    "@toNode":"Colorer"
                  }
                },
                { "ROUTE":
                  {
                    "@fromField":"value_changed",
                    "@fromNode":"Colorer",
                    "@toField":"diffuseColor",
                    "@toNode":"Pulsar"
                  }
                }
              ]
            }
          }
        ]
    }
